doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
    script(src="https://cdn.jsdelivr.net/npm/html-docx-js@0.3.1/dist/html-docx.js")
  body 
    script 
      :coffee
        html = "<h1>Judul</h1><p>This is a paragraph.</p>"
        hasil = htmlDocx.asBlob(html)
        # console.log hasil

        a = document.createElement "a"
        a.href = URL.createObjectURL hasil
        a.download = "hasil.docx"
        a.click()